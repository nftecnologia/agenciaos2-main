{
  "name": "agenciaos-worker",
  "source": {
    "type": "github",
    "repo": "nftecnologia/agenciaos2-main",
    "branch": "main"
  },
  "build": {
    "builder": "nixpacks"
  },
  "deploy": {
    "startCommand": "npm run worker",
    "healthcheckPath": "/health",
    "restartPolicyType": "always"
  },
  "environment": {
    "NODE_ENV": "production",
    "REDIS_URL": "${{Redis.REDIS_URL}}",
    "DATABASE_URL": "${{Postgres.DATABASE_URL}}"
  }
}